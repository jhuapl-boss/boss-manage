        "redirectUris": ["/openid/callback/login/","/openid/callback/logout/"],

        "consentRequired": false,
        "publicClient": true,
        "defaultClientScopes": [
            "web-origins",
            "role_list",
            "roles",
            "profile",
            "email"
        ],

        "directAccessGrantsEnabled": true
    },{
        "enabled": true,
        "protocol": "openid-connect",

        "clientId": "church",
        "name": "BOSS Church",
        "description": "BOSS Church team OpenID Connect Client",

        "redirectUris": ["/openid/callback/login/","/openid/callback/logout/"],

        "consentRequired": false,
        "publicClient": true,

        "directAccessGrantsEnabled": true
    }],

    "defaultDefaultClientScopes": [
        "email",
        "web-origins",
        "role_list",
        "roles",
        "profile"
    ],

    "roles": {
        "realm":[
            {"name":"resource-manager"},
            {"name":"user-manager"},
            {"name":"admin"},
            {"name":"superuser"}
        ]
    },

    "users": [{
        "username": "xxxxxx",
        "credentials": [{
            "type": "password",
